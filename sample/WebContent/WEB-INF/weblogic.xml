<weblogic-web-app>
	<security-role-assignment>
		<role-name>AllUsers</role-name>
		<principal-name>users</principal-name>
	</security-role-assignment>
	<session-descriptor>
		<persistent-store-type>replicated</persistent-store-type>
		<debug-enabled>true</debug-enabled>
	</session-descriptor>
</weblogic-web-app>